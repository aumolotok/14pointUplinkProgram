<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_IUploadService" />
                <binding name="BasicHttpBinding_IUploadService1">
                    <security mode="Transport" />
                </binding>
            </basicHttpBinding>
            <wsHttpBinding>
                <binding name="WSHttpBinding_IUploadService">
                    <security mode="None" />
                </binding>
                <binding name="WSHttpBinding_IUploadService1">
                    <security mode="Transport">
                        <transport clientCredentialType="None" />
                    </security>
                </binding>
            </wsHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://app-la-web2/Upload.svc/stream" binding="basicHttpBinding"
                bindingConfiguration="BasicHttpBinding_IUploadService" contract="UploadService.IUploadService"
                name="BasicHttpBinding_IUploadService" />
            <endpoint address="https://appulatebeta.com/Upload.svc/stream"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IUploadService1"
                contract="UploadService.IUploadService" name="BasicHttpBinding_IUploadService1" />
            <endpoint address="http://app-la-web2/Upload.svc" binding="wsHttpBinding"
                bindingConfiguration="WSHttpBinding_IUploadService" contract="UploadService.IUploadService"
                name="WSHttpBinding_IUploadService" />
            <endpoint address="https://appulatebeta.com/Upload.svc" binding="wsHttpBinding"
                bindingConfiguration="WSHttpBinding_IUploadService1" contract="UploadService.IUploadService"
                name="WSHttpBinding_IUploadService1" />
        </client>
    </system.serviceModel>
</configuration>